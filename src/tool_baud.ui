<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <requires lib="Adw" version="1.0"/>
  <template class="TaunoToolBaudWindow" parent="AdwWindow">
    <property name="title" translatable="yes">Find the Baud Rate</property>
    <property name="modal">true</property>
    <property name="default-width">300</property>
    <property name="default-height">300</property>
    <child>
      <object class="GtkBox">
        <property name="orientation">1</property>
        <property name="margin-top">20</property>
        <property name="margin-end">20</property>
        <property name="margin-bottom">20</property>
        <property name="margin-start">20</property>
        <property name="spacing">20</property>
        <child>
          <object class="GtkButton">
            <property name="label" translatable="yes">Find the Baud Rate</property>
            <signal name="clicked" handler="on_find_baud_rate"/>
          </object>
        </child>

        <child>
          <object class="GtkLabel" id="txt_1_label">
            <property name="label" translatable="yes">Siin peaks olema animatsioon.</property>
            <property name="xalign">0</property>
          </object>
        </child>

        <child>
          <object class="GtkButton">
            <property name="label" translatable="yes">Set this baud rate</property>
            <signal name="clicked" handler="on_set_baud_rate"/>
          </object>
        </child>

        <child>
          <object class="GtkButton">
            <property name="label" translatable="yes">Close window</property>
            <signal name="clicked" handler="on_button_close_clicked"/>
          </object>
        </child>
      </object>
    </child>
  </template>
  <object class="GtkTextBuffer" id="text_buffer"></object>
</interface>
